{
  "name": "contelix",
  "version": "0.0.1",
  "repository": "https://github.com/contelix/contelix.git",
  "author": "Martin Karry <martin@karry.de>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "turbo run start",
    "test": "turbo run test",
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "ui:start": "yarn workspace @contelix/ui run start",
    "ui:build": "yarn workspace @contelix/ui run build",
    "ui:test": "yarn workspace @contelix/ui run test",
    "ui:dev": "yarn workspace @contelix/ui run dev",
    "services:start": "yarn workspace @contelix/services run start",
    "services:build": "yarn workspace @contelix/services run build",
    "services:test": "yarn workspace @contelix/services run test",
    "services:dev": "yarn workspace @contelix/services run dev"
  },
  "devDependencies": {
    "turbo": "^1.10.12",
    "yarn": "^1.22.19"
  }
}
